{
    "name": "sglang",
    "remoteUser": "devuser",
    "dockerComposeFile": [
        "./docker-compose-observability.yml"
    ],
    "service": "sglang-dev",
    "runServices": [
        "sglang-dev"
    ],
    "workspaceFolder": "/workspaces/sglang",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "charliermarsh.ruff",
                "rust-lang.rust-analyzer",
                "tamasfe.even-better-toml"
            ]
        }
    },
    "remoteEnv": {
        "HOME": "/home/devuser"
    },
    "postCreateCommand": "/bin/bash .devcontainer/post-create.sh"
}
